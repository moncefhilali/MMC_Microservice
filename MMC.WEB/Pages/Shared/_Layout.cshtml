<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MMC.WEB</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/MMC.WEB.styles.css" asp-append-version="true" />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>

    <!-- Fontfaces CSS-->
    <link href="~/css/font-face.css" rel="stylesheet" media="all">
    <link href="~/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">

    <!-- Bootstrap CSS-->
    <link href="~/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

    <!-- Vendor CSS-->
    <link href="~/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/wow/animate.css" rel="stylesheet" media="all">
    <link href="~/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/slick/slick.css" rel="stylesheet" media="all">
    <link href="~/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">

    <!-- Main CSS-->
    <link href="~/css/theme.css" rel="stylesheet" media="all">

    <!-- Bootstrap Icons -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.1/font/bootstrap-icons.css">

    <!-- Bootstrap toggle switch -->
    <link href="https://cdn.jsdelivr.net/gh/gitbrent/bootstrap4-toggle@3.6.1/css/bootstrap4-toggle.min.css" rel="stylesheet">
</head>
<body>
    <div class="page-wrapper">
        <!-- HEADER MOBILE-->
        <header class="header-mobile d-block d-lg-none">
            <div class="header-mobile__bar">
                <div class="container-fluid">
                    <div class="header-mobile-inner">
                        <a href="#">
                            <span class="h2 text text-dark">Morocco Microsoft Community</span>
                        </a>
                        <button class="hamburger hamburger--slider" type="button">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>
            <nav class="navbar-mobile">
                <div class="container-fluid">
                    <ul class="navbar-mobile__list list-unstyled">

                        <li>
                            <a asp-page="/Index">
                                <i class="fas fa-chart-bar"></i>Dashboard
                            </a>
                        </li>

                        <li class="h6 has-sub">
                            <a class="js-arrow" href="#">
                                <i class="bi bi-calendar-event-fill"></i>Events
                            </a>
                            <ul class="navbar-mobile-sub__list list-unstyled js-sub-list">
                                <li>
                                    <a asp-page="/Event/Create" style="font-size:15px">
                                        <i class="bi bi-calendar2-plus-fill"></i>New Event
                                    </a>
                                </li>
                                <li>
                                    <a asp-page="/Event/Index" style="font-size:15px">
                                        <i class="bi bi-calendar2-range-fill"></i>Events List
                                    </a>
                                </li>
                                <li>
                                    <a asp-page="/Event/Session/Index" style="font-size:15px">
                                        <i class="bi bi-calendar2-week-fill"></i>New Event Session
                                    </a>
                                </li>
                            </ul>
                        </li>

                        <li>
                            <a asp-page="/Modules/Index">
                                <i class="bi bi-pen-fill"></i>Events
                            </a>
                        </li>

                        <li>
                            <a asp-page="/Courses/Index">
                                <i class="bi bi-book-half"></i>Speakers
                            </a>
                        </li>

                        <li>
                            <a asp-page="/Groups/Index">
                                <i class="bi bi-box-fill"></i>Sponsors
                            </a>
                        </li>

                        <li>
                            <a asp-page="/Salles/Index">
                                <i class="bi bi-building-fill"></i>Participants
                            </a>
                        </li>

                        <li>
                            <a asp-page="/Teachers/Index">
                                <i class="bi bi-mortarboard-fill"></i>Users
                            </a>
                        </li>

                        <li class="h6 has-sub">
                            <a class="js-arrow" href="#">
                                <i class="bi bi-sliders"></i>Settings
                            </a>
                            <ul class="navbar-mobile-sub__list list-unstyled js-sub-list">
                                <li>
                                    <a asp-page="/Settings/City/Index" style="font-size:15px">
                                        <i class="bi bi-geo-alt-fill"></i>Cities
                                    </a>
                                </li>
                                <li>
                                    <a asp-page="/Settings/Categorie/Index" style="font-size:15px">
                                        <i class="bi bi-bookmarks-fill"></i>Event Categories
                                    </a>
                                </li>
                            </ul>
                        </li>


                    </ul>
                </div>
            </nav>
        </header>
        <!-- END HEADER MOBILE-->
        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar d-none d-lg-block">
            <div class="logo">
                <a href="#">
                    <img src="https://cdn2.iconfinder.com/data/icons/hats-n-caps-2/128/hat_cap_clothing_fashion-10-512.png" style="height:40px; width:40px" />
                    <span class="h4 text text-dark">MMC</span>

                </a>
            </div>
            <div class="menu-sidebar__content js-scrollbar1">
                <nav class="navbar-sidebar">
                    <ul class="list-unstyled navbar__list">

                        <li class="h6">
                            <a asp-page="/Index">
                                <i class="fas fa-chart-bar"></i>Dashboard
                            </a>
                        </li>

                        <li class="h6 has-sub">
                            <a class="js-arrow" href="#">
                                <i class="bi bi-calendar-event-fill"></i>Events
                            </a>
                            <ul class="list-unstyled navbar__sub-list js-sub-list">
                                <li>
                                    <a asp-page="/Event/Create" style="font-size:15px">
                                        <i class="bi bi-calendar2-plus-fill"></i>New Event
                                    </a>
                                </li>
                                <li>
                                    <a asp-page="/Event/Index" style="font-size:15px">
                                        <i class="bi bi-calendar2-range-fill"></i>Events List
                                    </a>
                                </li>
                                <li>
                                    <a asp-page="/Event/Session/Index" style="font-size:15px">
                                        <i class="bi bi-calendar2-week-fill"></i>New Event Session
                                    </a>
                                </li>
                            </ul>
                        </li>

                        <li class="h6">
                            <a asp-page="/Courses/Index">
                                <i class="bi bi-megaphone-fill"></i>Speakers
                            </a>
                        </li>

                        <li class="h6">
                            <a asp-page="/Groups/Index">
                                <i class="bi bi-cash-coin"></i>Sponsors
                            </a>
                        </li>

                        <li class="h6">
                            <a asp-page="/Salles/Index">
                                <i class="bi bi-people-fill"></i>Participants
                            </a>
                        </li>

                        <li class="h6">
                            <a asp-page="/Teachers/Index">
                                <i class="bi bi-person-circle"></i>Users
                            </a>
                        </li>
                        
                        <li class="h6 has-sub">
                            <a class="js-arrow" href="#">
                                <i class="bi bi-sliders"></i>Settings
                            </a>
                            <ul class="list-unstyled navbar__sub-list js-sub-list">
                                <li>
                                    <a asp-page="/Cities/Index" style="font-size:15px">
                                        <i class="bi bi-geo-alt-fill"></i>Cities
                                    </a>
                                </li>
                                <li>
                                    <a asp-page="/Settings/Category/Index" style="font-size:15px">
                                        <i class="bi bi-bookmarks-fill"></i>Event Categories
                                    </a>
                                </li>
                            </ul>
                        </li>

                        <!---------------------------------------------------------------------------------------------->



                    </ul>
                </nav>
            </div>
        </aside>
        <!-- END MENU SIDEBAR-->
        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <header class="header-desktop">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap">
                            <form class="form-header" action="" method="POST">
                                <input class="au-input au-input--xl" type="text" name="search" placeholder="Search for events & speakers..." />
                                <button class="au-btn--submit" type="submit">
                                    <i class="zmdi zmdi-search"></i>
                                </button>
                            </form>
                        </div>
                    </div>
                </div>
            </header>
            <!-- HEADER DESKTOP-->
            <!-- MAIN CONTENT-->
            <div class="main-content">
                @RenderBody()
            </div>
        </div>

        <!-- Jquery JS-->
        <script src="~/vendor/jquery-3.2.1.min.js"></script>
        <!-- Bootstrap JS-->
        <script src="~/vendor/bootstrap-4.1/popper.min.js"></script>
        <script src="~/vendor/bootstrap-4.1/bootstrap.min.js"></script>
        <!-- Vendor JS       -->
        <script src="~/vendor/slick/slick.min.js"></script>
        <script src="~/vendor/wow/wow.min.js"></script>
        <script src="~/vendor/animsition/animsition.min.js"></script>
        <script src="~/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
        <script src="~/vendor/counter-up/jquery.waypoints.min.js"></script>
        <script src="~/vendor/counter-up/jquery.counterup.min.js"></script>
        <script src="~/vendor/circle-progress/circle-progress.min.js"></script>
        <script src="~/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
        <script src="~/vendor/chartjs/Chart.bundle.min.js"></script>
        <script src="~/vendor/select2/select2.min.js"></script>
        <!-- Main JS-->
        <script src="~/js/main.js"></script>
        <!-- Bootstrap toggle switch -->
        <script src="https://cdn.jsdelivr.net/gh/gitbrent/bootstrap4-toggle@3.6.1/js/bootstrap4-toggle.min.js"></script>


        @RenderSection("Scripts", required: false)
        @RenderSection("DocUploadJS", false);
        @RenderSection("InputsPattern", false);
    </div>


    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - MMC.WEB - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
</body>
</html>